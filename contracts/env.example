# Smart Contract Environment Variables
# Copy this to .env: cp env.example .env

# Deployment Private Key
PRIVATE_KEY=0x...

# Arc Testnet RPC
ARC_TESTNET_RPC_URL=https://rpc.testnet.arc.circle.com

# Aegis Server Wallet
AEGIS_SERVER_WALLET=0x...

# Arc Explorer API Key (for verification)
ARC_EXPLORER_API_KEY=your_explorer_api_key

# Initial Liquidity (optional, in USDC with 6 decimals)
# Example: 10000000000 = 10,000 USDC
INITIAL_LIQUIDITY=0

